{
	"productName": "pomodoro-perron",
	"version": "0.0.0",
	"identifier": "com.tauri.dev",
	"build": {
		"frontendDist": "../src"
	},
	"app": {
		"withGlobalTauri": true,
		"windows": [
			{
				"fullscreen": false,
				"resizable": true,
				"title": "pomodoro-perron",
				"width": 800,
				"height": 600
			}
		],
		"security": {
			"csp": "default-src 'self'; img-src *; media-src *; asset: *"
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
